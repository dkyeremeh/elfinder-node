{
  "name": "elfinder-node",
  "version": "1.0.3",
  "description": "A NodeJS connector/backend for elFinder file manager",
  "main": "dist/elfinder.js",
  "types": "dist/elfinder.d.ts",
  "author": "d.kyeremeh@dekyfin.com",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "archiver": "^1.2.0",
    "express": "^4.15.2",
    "express-busboy": "^10.1.0",
    "fs-extra": "^11.1.1",
    "jimp": "^0.2.27",
    "lzutf8": "^0.3.5",
    "mime-types": "^2.1.12",
    "promise": "^8.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.0",
    "@types/underscore": "^1.13.0",
    "ava": "^5.3.0",
    "c8": "^7.14.0",
    "eslint": "^8.41.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "qs": "^6.11.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "serve": "nodemon tests/server.js",
    "test:int": "npm run build && ava tests/int-tests/**/*.js",
    "test:cov": "npm run build && c8 -r text -r json-summary  ava tests/int-tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dekyfin/elfinder-node.git"
  },
  "keywords": [
    "file manager",
    "elFinder",
    "nodejs",
    "connector"
  ],
  "nodemonConfig": {
    "ignore": [
      "media/*",
      "tests/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/dekyfin/elfinder-node/issues"
  },
  "homepage": "https://github.com/dekyfin/elfinder-node#readme"
}
